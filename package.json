{
  "name": "gulp-starter",
  "version": "0.1.1",
  "description": "Gulp starter with common tasks and scenarios",
  "repository": {
    "type": "git",
    "url": "git://github.com/greypants/gulp-starter.git"
  },
  "browser": {
    "underscore": "backbone/node_modules/underscore",
    "plugin": "./src/javascript/vendor/jquery-plugin.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "hbsfy"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "~1.3.6",
    "browserify": "~5.10.1",
    "browserify-shim": "~3.7.0",
    "gulp": "^3.8.7",
    "gulp-autoprefixer": "^2.0.0",
    "gulp-cached": "^1.0.1",
    "gulp-changed": "^0.4.1",
    "gulp-imagemin": "^0.6.2",
    "gulp-insert": "^0.4.0",
    "gulp-jscs": "^1.3.0",
    "gulp-jshint": "^1.9.0",
    "gulp-notify": "^1.4.2",
    "gulp-sass": "^1.1.0",
    "gulp-sourcemaps": "^1.2.8",
    "gulp-util": "^3.0.0",
    "handlebars": "^1.3.0",
    "hbsfy": "~2.0.0",
    "jstransform": "^7.0.0",
    "jstransform-loader": "^0.1.1",
    "jsx-loader": "^0.11.2",
    "nuclear-js": "^0.5.0-rc1",
    "pretty-hrtime": "~0.2.1",
    "require-dir": "^0.1.0",
    "sass-list-maps": "^1.0.0-b",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~1.0.2",
    "webpack": "^1.4.8"
  },
  "dependencies": {
    "backbone": "~1.1.2",
    "jquery": "~2.1.0",
    "react": "^0.11.2",
    "immutable": "^3.0.x",
    "nuclear-react-mixin": "^3.0.0",
    "lodash": "^2.4.1",
    "uuid": "^2.0.1",
    "keymirror": "^0.1.0"
  }
}
